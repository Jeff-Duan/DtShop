public class PhoService {
	PhoImpl phoImpl = new PhoImpl();
	public PhoPhoto getPhoPhoto(String id) {
		
		PhoPhoto phoPhoto = phoImpl.selectPhoto(id);
		return phoPhoto;
	}
	public int addCar(CarCar car){	
		int i =phoImpl.addCar(car);

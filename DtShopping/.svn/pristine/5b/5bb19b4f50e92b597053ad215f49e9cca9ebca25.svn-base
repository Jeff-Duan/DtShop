<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE>
<html>
<head>
<title>修改管理员信息</title>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<link href="${pageContext.request.contextPath}/static/css/after/adminStyle.css" rel="stylesheet" type="text/css" />
<script src="${pageContext.request.contextPath}/static/js/after/jquery.min.js"></script>
<script type="text/javascript" src="${pageContext.request.contextPath}/static/js/after/laydate/laydate.js"></script>
<script src="${pageContext.request.contextPath}/static/js/after/public.js"></script>
<script type="text/javascript" src="${pageContext.request.contextPath}/static/js/after/wangEditor.min.js"></script>
</head>
<body>
 <div class="wrap">
  <div class="page-title">
    <span class="modular fl"><img alt="修改密码" src="${pageContext.request.contextPath}/static/img/after/修改密码.png"> <em>修改密码</em></span>
  </div>
  <form action="/DtShopping/updateManagerPasswordServlet"  method="post" id="uploadForm">
  <table class="list-style">
     <tr>
   <td style="text-align:right;width:15%;">请输入您的账号：</td>
    <td><input type="text"  class="textBox length-long"  name="username" "/></td>
   </tr>
   <tr>
   <td style="text-align:right;width:15%;">请输入旧密码：</td>
    <td><input type="password"  class="textBox length-long"  name="oldpassword" "/></td>
   </tr>
   <tr>
    <td style="text-align:right;width:15%;">请输入新密码：</td>
    <td>
     <input type="password"  class="textBox length-long" name="newPassword1" "/>
    </td>
   </tr>
   <tr>
    <td style="text-align:right;width:15%">确认新密码：</td>
    <td>
     <input type="password"  class="textBox length-long" name="newPassword" "/>
    </td>
   </tr>
   <tr>
    <td style="text-align:right;"></td>
    <td><input id="mybtn" type="submit" value="提交" /></td>
   </tr>
  </table>
  </form>
 </div>
</body>
</html>